{
    "manifest_version": 3,
    "name": "EpohCSDN",
    "version": "1.1",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.csdn.net/*",
                "https://blog.csdn.net/*"
            ],
            "run_at": "document_end",
            "js": [
                "document_end.js"
            ]
        }
    ]
}