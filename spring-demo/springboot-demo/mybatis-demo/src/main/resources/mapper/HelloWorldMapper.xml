<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.net.bhe.mybatisdemo.mapper.HelloWorldMapper">

    <select id="selectHelloWorld" resultType="String">
        select 'Hello World!'
        from dual
    </select>

    <select id="selectSeqHelloWorldNextval" resultType="Long">
        select seq_hello_world.nextval
        from dual
    </select>

</mapper>